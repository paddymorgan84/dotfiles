{
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.language": "en-GB",
  "diffEditor.ignoreTrimWhitespace": false,
  "dotfiles.installCommand": "~/dotfiles/setup.sh",
  "dotfiles.repository": "paddymorgan84/dotfiles",
  "dotfiles.targetPath": "~/dotfiles",
  "editor.codeLens": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "extensions.ignoreRecommendations": false,
  "files.associations": {
    "*.Jenkinsfile": "groovy"
  },
  "files.autoSave": "onFocusChange",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "go.formatTool": "gofmt",
  "go.useLanguageServer": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "python.formatting.autopep8Args": ["--max-line-length", "300"],
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "search.exclude": {
    "**/node_modules": true
  },
  "sync.gist": "9e0c44bc946e03f885ef58a9d731b861",
  "terminal.integrated.fontFamily": "Fira Code",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.restoreWindows": "none",
  "window.title": "${dirty}${rootName}${separator}${activeFolderMedium}/${activeEditorShort}",
  "window.zoomLevel": -1,
  "workbench.colorTheme": "Dracula",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editorAssociations": [
    {
      "filenamePattern": "git-rebase-todo",
      "viewType": "default"
    }
  ],
  "workbench.iconTheme": "material-icon-theme"
}
